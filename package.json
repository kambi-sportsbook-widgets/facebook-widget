{
  "name": "facebook-widget",
  "version": "1.0.2",
  "description": "Facebook embeded pages widget",
  "main": "dist/index.html",
  "scripts": {
    "build:prod": "webpack --config ./node_modules/widget-build-tools/webpack.production.config.js --verbose --display-modules --display-error-details",
    "build:dev": "webpack-dev-server --debug --devtool source-map --output-pathinfo --hot --watch --inline --config ./node_modules/widget-build-tools/webpack.development.config.js",
    "clean": "kambi-widgets-cli clean",
    "start": "kambi-widgets-cli build --dev",
    "build": "kambi-widgets-cli build --prod",
    "preversion": "kambi-widgets-cli preversion",
    "postversion": "kambi-widgets-cli postversion"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kambi-sportsbook-widgets/facebook-widget.git"
  },
  "dependencies": {
    "widget-core-library": "git+ssh://git@github.com/kambi-sportsbook-widgets/widget-core-library.git#KSBWI-742"
  },
  "devDependencies": {
    "widget-build-tools": "git+ssh://git@github.com/kambi-sportsbook-widgets/widget-build-tools.git#KSBWI-710"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kambi-sportsbook-widgets/facebook-widget/issues"
  },
  "homepage": "https://github.com/kambi-sportsbook-widgets/facebook-widget#readme"
}
